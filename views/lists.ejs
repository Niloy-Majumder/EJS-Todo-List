<%- include('header'); -%>

<div class="box" id="heading">
  <h1>It's a <%= renderday%></h1>
</div>
<div class="box">

  <%  for(var i=0;i<item.length;i++){ %>
  <div class="item">
    <input type="checkbox">
    <p><%= item[i]%></p>
  </div>
  <%  } %>


  <form class="item" action="/" method="post">
    <input name="listitem" id="List" type="text" placeholder="Add Items" autocomplete="off" />
    <button type="submit" name="button" value="<%= renderday%>">+</button>
  </form>
</div>
<%- include('footer'); -%>